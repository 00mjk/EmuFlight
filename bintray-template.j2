{
    "files": [
        {
            "includePattern": "obj/(.*\\.hex)",
            "uploadPattern": "{{ env("BINTRAY_VERSION") }}/$1",
            "matrixParams": { "override": 1 }
        }
    ],
    "package": {
        "subject": "emuflight",
        "repo": "dev_hex",
        "desc": "do emus dream of flying?",
        "issue_tracker_url": "https://github.com/emuflight/EmuFlight-Butter-Varient/issues",
        "website_url": "https://github.com/emuflight/EmuFlight-Butter-Varient",
        "public_download_numbers": false,
        "public_stats": false,
        "licenses": [
            "GPL-3.0"
        ],
        "labels": [
            "emuflight-{{ env("EMU_VERSION") }}",
            "emuflight"
        ],
        "name": "{{ env("TRAVIS_BRANCH") }}",
        "vcs_url": "{{ env("TRAVIS_BUILD_WEB_URL") }}"
    },
    "publish": true,
    "version": {
        "gpgSign": false,
        "name": "{{ env("BINTRAY_VERSION") }}",
        "desc": "Emuflight Development Release - changes in this build: {{ env("TRAVIS_COMMIT_MESSAGE") }}"
    }
}